# Vision API Configuration
# You need at least one of these API keys

# Anthropic (Claude)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI (GPT-4 Vision)
OPENAI_API_KEY=your_openai_api_key_here

# Model Configuration
# Primary model to use: "claude" or "openai"
PRIMARY_MODEL=claude

# Fallback model to use if primary fails: "claude", "openai", or "none"
FALLBACK_MODEL=openai

# Specific model versions (optional)
CLAUDE_MODEL=claude-sonnet-4-5-20250929
OPENAI_MODEL=gpt-4-vision-preview

# API Server Configuration (for FastAPI)
API_HOST=0.0.0.0
API_PORT=8000

# Logging
LOG_LEVEL=INFO
